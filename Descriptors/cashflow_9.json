{
  "id": "urn:dmb:dp:finance:cashflow_9:0",
  "name": "cashflow_9",
  "version": "0.3.9",
  "fullyQualifiedName": "Liquidity Cash Flows v3 \u2014 9",
  "domain": "finance/liquidity",
  "tags": [
    "finance",
    "gdpr",
    "restricted"
  ],
  "orchestration": {
    "tool": "dbt",
    "details": {
      "models": [
        "stg_cashflow",
        "dim_cashflow"
      ]
    }
  },
  "qualityControls": {
    "framework": "GreatExpectations",
    "controls": [
      {
        "expectation": "expect_column_values_to_not_be_null",
        "columns": [
          "RecordID"
        ]
      },
      {
        "expectation": "expect_column_values_to_be_between",
        "column": "NetAmount",
        "min": 0
      },
      {
        "expectation": "expect_column_values_to_be_in_set",
        "column": "FlowType",
        "set": [
          "IN",
          "OUT"
        ]
      }
    ],
    "additionalFrameworks": [
      "Deequ"
    ]
  },
  "workloads": [
    {
      "name": "purge_personal_data",
      "type": "lambda",
      "description": "Implements right to be forgotten by purging matching records."
    }
  ],
  "metadata": {
    "owner": {
      "team": "Treasury IT",
      "contact": "treasury-engineering@example.org"
    },
    "costCenter": "FIN-CF",
    "classification": "restricted",
    "description": "Data product delivering cash movement facts; non-sensitive aggregates only."
  },
  "observability": {
    "metrics": [
      "row_count",
      "null_rate",
      "latency"
    ],
    "logging": {
      "level": "DEBUG",
      "retentionDays": 14
    }
  },
  "lineage": {
    "upstreamSources": [
      {
        "system": "ERP",
        "object": "GL_Bookings"
      },
      {
        "system": "BankAPI",
        "object": "Movements"
      }
    ]
  },
  "dataContracts": [
    {
      "id": "urn:dmb:dp:finance:cashflow_9:0:snowflaketable_output",
      "type": "SnowflakeTable",
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING",
          "description": "Unique internal identifier for a cash flow row."
        },
        {
          "name": "NetAmount",
          "dataType": "DECIMAL(18,2)",
          "description": "Monetary movement in EUR; non-negative."
        },
        {
          "name": "FlowType",
          "dataType": "STRING",
          "description": "IN for incoming cash, OUT for outgoing cash."
        },
        {
          "name": "PostingDate",
          "dataType": "DATE",
          "description": "Ledger posting date."
        }
      ],
      "columnTags": {
        "FlowType": {
          "businessTerms": [
            "FundingSource",
            "TreasuryActivity",
            "CounterpartyCategory"
          ]
        }
      },
      "sla": {
        "intervalOfChange": "P1D",
        "timeliness": "PT30M",
        "upTime": 0.987
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_9:0:kafka_output",
      "type": "KafkaTopic",
      "topic": "liquidity.cashflow.variant.9",
      "partitions": 5,
      "retentionDays": 14,
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING"
        },
        {
          "name": "NetAmount",
          "dataType": "DOUBLE"
        },
        {
          "name": "FlowType",
          "dataType": "STRING"
        },
        {
          "name": "IngestedAt",
          "dataType": "TIMESTAMP"
        }
      ],
      "columnTags": {
        "FlowType": {
          "businessTerms": [
            "AccountingPeriod"
          ]
        }
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_9:0:s3parquet_aux",
      "type": "S3Parquet",
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING",
          "description": "Primary event identifier."
        },
        {
          "name": "NetAmount",
          "dataType": "NUMERIC",
          "description": "Amount in EUR, zero or positive."
        },
        {
          "name": "FlowType",
          "dataType": "STRING",
          "description": "Direction of cash flow."
        },
        {
          "name": "PostingDate",
          "dataType": "DATE",
          "description": "Accounting date."
        },
        {
          "name": "IngestedAt",
          "dataType": "TIMESTAMP",
          "description": "Event timestamp."
        }
      ],
      "sla": {
        "intervalOfChange": "PT6H",
        "timeliness": "PT1H",
        "upTime": 0.986
      }
    }
  ],
  "notes": [
    "Variant 9: topic=liquidity.cashflow.variant.9, partitions=5, retention=14d.",
    "Descriptions in streaming schema are intentionally absent to test contract completeness checks.",
    "No PII is modeled; GDPR tag remains at product level only."
  ]
}