{
  "id": "urn:dmb:dp:finance:cashflow_5:0",
  "name": "cashflow_5",
  "version": "0.3.5",
  "fullyQualifiedName": "Liquidity Cash Flows v3 \u2014 5",
  "domain": "finance/liquidity",
  "tags": [
    "finance",
    "gdpr",
    "internal"
  ],
  "orchestration": {
    "tool": "dbt+Prefect",
    "details": {
      "schedules": [
        "0 6 * * *"
      ]
    }
  },
  "qualityControls": {
    "framework": "GreatExpectations",
    "controls": [
      {
        "expectation": "expect_column_values_to_not_be_null",
        "columns": [
          "RecordID"
        ]
      },
      {
        "expectation": "expect_column_values_to_be_between",
        "column": "NetAmount",
        "min": 0
      },
      {
        "expectation": "expect_column_values_to_be_in_set",
        "column": "FlowType",
        "set": [
          "IN",
          "OUT"
        ]
      },
      {
        "expectation": "expect_table_row_count_to_be_between",
        "min": 1
      }
    ],
    "additionalFrameworks": [
      "Deequ"
    ]
  },
  "workloads": [
    {
      "name": "purge_personal_data",
      "type": "lambda",
      "description": "Implements right to be forgotten by purging matching records."
    }
  ],
  "metadata": {
    "owner": {
      "team": "Treasury IT",
      "contact": "treasury-engineering@example.org"
    },
    "costCenter": "FIN-TRS",
    "classification": "restricted",
    "description": "Data product delivering cash movement facts; non-sensitive aggregates only."
  },
  "observability": {
    "metrics": [
      "row_count",
      "null_rate",
      "latency"
    ],
    "logging": {
      "level": "DEBUG",
      "retentionDays": 14
    }
  },
  "lineage": {
    "upstreamSources": [
      {
        "system": "ERP",
        "object": "Treasury_Transfers"
      },
      {
        "system": "BankAPI",
        "object": "Movements"
      }
    ]
  },
  "dataContracts": [
    {
      "id": "urn:dmb:dp:finance:cashflow_5:0:bigquerytable_output",
      "type": "BigQueryTable",
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING",
          "description": "Unique internal identifier for a cash flow row."
        },
        {
          "name": "NetAmount",
          "dataType": "DECIMAL(18,2)",
          "description": "Monetary movement in EUR; non-negative."
        },
        {
          "name": "FlowType",
          "dataType": "STRING",
          "description": "IN for incoming cash, OUT for outgoing cash."
        },
        {
          "name": "PostingDate",
          "dataType": "DATE",
          "description": "Ledger posting date."
        }
      ],
      "columnTags": {
        "FlowType": {
          "businessTerms": [
            "CashFlowDirection",
            "PostingChannel",
            "SettlementMethod"
          ]
        }
      },
      "sla": {
        "intervalOfChange": "PT12H",
        "timeliness": "PT20M",
        "upTime": 0.985
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_5:0:kafka_output",
      "type": "KafkaTopic",
      "topic": "liquidity.cashflow.variant.5",
      "partitions": 7,
      "retentionDays": 10,
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING"
        },
        {
          "name": "NetAmount",
          "dataType": "DOUBLE"
        },
        {
          "name": "FlowType",
          "dataType": "STRING"
        },
        {
          "name": "IngestedAt",
          "dataType": "TIMESTAMP"
        }
      ],
      "columnTags": {
        "FlowType": {
          "businessTerms": [
            "PostingChannel",
            "LiquidityStage"
          ]
        }
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_5:0:bigquerytable_aux",
      "type": "BigQueryTable",
      "schema": [
        {
          "name": "RecordID",
          "dataType": "STRING",
          "description": "Primary event identifier."
        },
        {
          "name": "NetAmount",
          "dataType": "NUMERIC",
          "description": "Amount in EUR, zero or positive."
        },
        {
          "name": "FlowType",
          "dataType": "STRING",
          "description": "Direction of cash flow."
        },
        {
          "name": "PostingDate",
          "dataType": "DATE",
          "description": "Accounting date."
        },
        {
          "name": "IngestedAt",
          "dataType": "TIMESTAMP",
          "description": "Event timestamp."
        }
      ],
      "sla": {
        "intervalOfChange": "P2D",
        "timeliness": "PT45M",
        "upTime": 0.986
      }
    }
  ],
  "notes": [
    "Variant 5: topic=liquidity.cashflow.variant.5, partitions=7, retention=10d.",
    "Descriptions in streaming schema are intentionally absent to test contract completeness checks.",
    "No PII is modeled; GDPR tag remains at product level only."
  ]
}