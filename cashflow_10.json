{
  "id": "urn:dmb:dp:finance:cashflow_10:0",
  "name": "cashflow_10",
  "version": "0.3.10",
  "fullyQualifiedName": "Liquidity Cash Flows v3 \u2014 10",
  "domain": "finance/liquidity",
  "tags": [
    "finance",
    "gdpr",
    "restricted"
  ],
  "orchestration": {
    "tool": "dbt+Prefect",
    "details": {
      "schedules": [
        "0 6 * * *"
      ]
    }
  },
  "qualityControls": {
    "framework": "GreatExpectations",
    "controls": [
      {
        "expectation": "expect_column_values_to_not_be_null",
        "columns": [
          "RowKey"
        ]
      },
      {
        "expectation": "expect_column_values_to_be_between",
        "column": "AmountEUR",
        "min": 0
      },
      {
        "expectation": "expect_column_values_to_be_in_set",
        "column": "Direction",
        "set": [
          "IN",
          "OUT"
        ]
      },
      {
        "expectation": "expect_table_row_count_to_be_between",
        "min": 1
      }
    ]
  },
  "workloads": [
    {
      "name": "purge_personal_data",
      "type": "lambda",
      "description": "Implements right to be forgotten by purging matching records."
    }
  ],
  "metadata": {
    "owner": {
      "team": "Treasury IT",
      "contact": "treasury-engineering@example.org"
    },
    "costCenter": "FIN-CF",
    "classification": "internal",
    "description": "Data product delivering cash movement facts; non-sensitive aggregates only."
  },
  "observability": {
    "metrics": [
      "row_count",
      "null_rate",
      "latency"
    ],
    "logging": {
      "level": "INFO",
      "retentionDays": 14
    }
  },
  "lineage": {
    "upstreamSources": [
      {
        "system": "ERP",
        "object": "Payments"
      },
      {
        "system": "BankAPI",
        "object": "Statements"
      }
    ]
  },
  "dataContracts": [
    {
      "id": "urn:dmb:dp:finance:cashflow_10:0:snowflaketable_output",
      "type": "SnowflakeTable",
      "schema": [
        {
          "name": "RowKey",
          "dataType": "STRING",
          "description": "Unique internal identifier for a cash flow row."
        },
        {
          "name": "AmountEUR",
          "dataType": "DECIMAL(18,2)",
          "description": "Monetary movement in EUR; non-negative."
        },
        {
          "name": "Direction",
          "dataType": "STRING",
          "description": "IN for incoming cash, OUT for outgoing cash."
        },
        {
          "name": "BookDate",
          "dataType": "DATE",
          "description": "Ledger posting date."
        }
      ],
      "columnTags": {
        "Direction": {
          "businessTerms": [
            "SettlementMethod"
          ]
        }
      },
      "sla": {
        "intervalOfChange": "P1D",
        "timeliness": "PT20M",
        "upTime": 0.989
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_10:0:kafka_output",
      "type": "KafkaTopic",
      "topic": "liquidity.cashflow.variant.10",
      "partitions": 6,
      "retentionDays": 5,
      "schema": [
        {
          "name": "RowKey",
          "dataType": "STRING"
        },
        {
          "name": "AmountEUR",
          "dataType": "DOUBLE"
        },
        {
          "name": "Direction",
          "dataType": "STRING"
        },
        {
          "name": "EventTs",
          "dataType": "TIMESTAMP"
        }
      ],
      "columnTags": {
        "Direction": {
          "businessTerms": [
            "AccountingPeriod",
            "SettlementMethod",
            "CounterpartyCategory"
          ]
        }
      }
    },
    {
      "id": "urn:dmb:dp:finance:cashflow_10:0:s3parquet_aux",
      "type": "S3Parquet",
      "schema": [
        {
          "name": "RowKey",
          "dataType": "STRING",
          "description": "Primary event identifier."
        },
        {
          "name": "AmountEUR",
          "dataType": "NUMERIC",
          "description": "Amount in EUR, zero or positive."
        },
        {
          "name": "Direction",
          "dataType": "STRING",
          "description": "Direction of cash flow."
        },
        {
          "name": "BookDate",
          "dataType": "DATE",
          "description": "Accounting date."
        },
        {
          "name": "EventTs",
          "dataType": "TIMESTAMP",
          "description": "Event timestamp."
        }
      ],
      "sla": {
        "intervalOfChange": "P1D",
        "timeliness": "PT30M",
        "upTime": 0.983
      }
    }
  ],
  "notes": [
    "Variant 10: topic=liquidity.cashflow.variant.10, partitions=6, retention=5d.",
    "Descriptions in streaming schema are intentionally absent to test contract completeness checks.",
    "No PII is modeled; GDPR tag remains at product level only."
  ]
}